{
  "name": "artistic-blacksmithing",
  "private": true,
  "description": "artistic-blacksmithing company",
  "author": "Tomasz Karny <karny.tomasz@gmail.com>",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "i18n:generate": "node scripts/i18n/generate-key-mapping.js",
    "i18n:audit": "node scripts/i18n/audit-keys.js",
    "i18n:validate": "node scripts/i18n/validate-translations.js",
    "i18n:check": "node scripts/i18n/validate-i18n.js",
    "i18n:check:generate": "node scripts/i18n/validate-i18n.js --generate-types",
    "prepare": "husky install",
    "lighthouse": "LHCI_GITHUB_APP_TOKEN=$(grep LHCI_GITHUB_APP_TOKEN .env.lighthouse | cut -d '=' -f2) lhci autorun",
    "lighthouse:desktop": "LHCI_GITHUB_APP_TOKEN=$(grep LHCI_GITHUB_APP_TOKEN .env.lighthouse | cut -d '=' -f2) lhci autorun --collect.preset=desktop",
    "lighthouse:mobile": "LHCI_GITHUB_APP_TOKEN=$(grep LHCI_GITHUB_APP_TOKEN .env.lighthouse | cut -d '=' -f2) lhci autorun --collect.preset=mobile"
  },
  "lint-staged": {
    "locales/**/*.json": [
      "node scripts/i18n/pre-commit-check.js"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.11.0",
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-brands-svg-icons": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@react-google-maps/api": "^2.20.6",
    "ajv": "^8.17.1",
    "chalk": "^4.1.2",
    "dotenv": "^16.0.3",
    "encoding": "^0.1.13",
    "eslint": "8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-webpack-plugin": "4.0.1",
    "facepaint": "^1.2.1",
    "gatsby": "^5.13.1",
    "gatsby-image": "^3.11.0",
    "gatsby-plugin-emotion": "^8.13.0",
    "gatsby-plugin-image": "^3.14.0",
    "gatsby-plugin-react-helmet": "^6.13.0",
    "gatsby-plugin-react-i18next": "^3.0.1",
    "gatsby-plugin-robots-txt": "^1.8.0",
    "gatsby-plugin-sharp": "^5.13.0",
    "gatsby-plugin-sitemap": "^6.14.0",
    "gatsby-source-filesystem": "^5.13.0",
    "gatsby-transformer-sharp": "^5.13.0",
    "i18next": "^22.5.1",
    "i18next-browser-languagedetector": "^7.1.0",
    "i18next-http-backend": "^2.2.1",
    "prop-types": "15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-i18next": "^12.3.1",
    "react-icons": "^5.5.0",
    "react-photo-gallery": "^8.0.0",
    "yet-another-react-lightbox": "^3.21.8"
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@babel/eslint-parser": "^7.26.8",
    "@babel/preset-react": "^7.26.3",
    "@lhci/cli": "^0.14.0",
    "eslint-config-airbnb": "^19.0.4",
    "gatsby-plugin-eslint": "^4.0.4",
    "gatsby-plugin-root-import": "^2.0.9",
    "gatsby-plugin-stylelint": "^3.2.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.5.1",
    "prettier": "^3.2.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "engines": {
    "node": ">=20.10.0",
    "npm": ">=10.0.0"
  }
}
